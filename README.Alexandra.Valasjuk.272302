public class SumNumbers {
    public static void main(String[] args) {
        // Определяем два числа
        int num1 = 5;
        int num2 = 7;
        
        // Вычисляем сумму
        int sum = num1 + num2;
        
        // Выводим результат
        System.out.println("Сумма " + num1 + " и " + num2 + " равна " + sum);
    }
}

